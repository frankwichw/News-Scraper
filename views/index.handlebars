<div class="container">
<div class="section">

    <div class="row">
        <div class="col s12 center article-div">

            {{#each articles}}
            <div class="row">
                <div class="col s12 m12">
                    <div class="card">
                        <div class="card-content">
                            <h3 class='left-align headline-text'><a href='{{url}}'>{{title}}</a></h3>
                            <a class="btn-floating btn-large waves-effect waves-light red save-button">
                                <i class="material-icons" id='save-article'>add</i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}

        </div>

        </div>

    </div>

</div>
</div>